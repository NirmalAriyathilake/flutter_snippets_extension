{
  "name": "flutter-dart-snippets",
  "displayName": "flutter-snippets",
  "description": "Snippets for flutter development",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/NirmalAriyathilake/flutter_snippets_extension"
  },
  "publisher": "NirmalCode",
  "icon": "logo.png",
  "keywords": [
    "Flutter",
    "Dart",
    "Snippets"
  ],
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Snippets"
  ],
